{
    "name": "@elgato/icons",
    "version": "0.1.0",
    "description": "Icons used throughout the Elgato ecosystem.",
    "type": "module",
    "exports": {
        ".": {
            "default": "./dist/js/index.js",
            "types": "./dist/js/index.d.ts"
        },
        "./s": {
            "default": "./dist/js/icons/small/index.js",
            "types": "./dist/js/icons/small/index.d.ts"
        },
        "./m": {
            "default": "./dist/js/icons/medium/index.js",
            "types": "./dist/js/icons/medium/index.d.ts"
        },
        "./l": {
            "default": "./dist/js/icons/large/index.js",
            "types": "./dist/js/icons/large/index.d.ts"
        },
        "./react": {
            "default": "./dist/react/index.js",
            "types": "./dist/react/index.d.ts"
        }
    },
    "files": [
        "dist/",
        "svg/"
    ],
    "scripts": {
        "build": "npm run build:js && npm run build:react && npm run tsc",
        "build:ci": "tsc",
        "build:js": "node --experimental-strip-types scripts/svg-to-js.ts && prettier ./src/js/icons/ --write",
        "build:react": "./scripts/svg-to-react.sh",
        "tsc": "tsc",
        "lint": "prettier . --check",
        "lint:fix": "prettier . --write"
    },
    "keywords": [
        "elgato",
        "icons",
        "stream deck",
        "wave link",
        "camera hub"
    ],
    "author": {
        "name": "Elgato",
        "url": "https://www.elgato.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/elgatosf/icons.git"
    },
    "prettier": "@elgato/prettier-config",
    "devDependencies": {
        "@elgato/prettier-config": "^0.2.4",
        "@svgr/cli": "^8.1.0",
        "@tsconfig/node20": "^20.1.4",
        "@types/node": "^22.8.6",
        "@types/react": "^18.3.12",
        "react": "^18.3.1",
        "typescript": "^5.6.3"
    }
}
