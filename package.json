{
    "name": "@elgato/icons",
    "version": "0.0.0",
    "description": "Icons used throughout the Elgato ecosystem.",
    "type": "module",
    "exports": {
        "./s": {
            "default": "./dist/strings/s/index.js",
            "types": "./dist/strings/s/index.d.ts"
        },
        "./m": {
            "default": "./dist/strings/m/index.js",
            "types": "./dist/strings/m/index.d.ts"
        },
        "./l": {
            "default": "./dist/strings/l/index.js",
            "types": "./dist/strings/l/index.d.ts"
        },
        "./metadata": {
            "default": "./dist/metadata/index.js",
            "types": "./dist/metadata/index.d.ts"
        },
        "./react": {
            "default": "./dist/react/index.js",
            "types": "./dist/react/index.d.ts"
        }
    },
    "files": [
        "dist/",
        "svg/"
    ],
    "scripts": {
        "build": "npm run transform && npm run lint:fix && npm run tsc",
        "build:ci": "tsc",
        "lint": "prettier . --check",
        "lint:fix": "prettier . --write --log-level warn",
        "transform": "node --experimental-strip-types scripts/transform.ts",
        "tsc": "tsc"
    },
    "keywords": [
        "elgato",
        "icons",
        "stream deck",
        "wave link",
        "camera hub",
        "marketplace",
        "makers"
    ],
    "author": {
        "name": "Elgato",
        "url": "https://www.elgato.com"
    },
    "license": "MIT",
    "homepage": "https://github.com/elgatosf/icons#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/elgatosf/icons.git"
    },
    "prettier": "@elgato/prettier-config",
    "devDependencies": {
        "@elgato/prettier-config": "^0.2.4",
        "@svgr/core": "^8.1.0",
        "@svgr/plugin-jsx": "^8.1.0",
        "@svgr/plugin-svgo": "^8.1.0",
        "@tsconfig/node20": "^20.1.4",
        "@types/node": "^22.8.6",
        "@types/react": "^18.3.12",
        "ora": "^8.1.1",
        "react": "^18.3.1",
        "typescript": "^5.6.3"
    }
}
