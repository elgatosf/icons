{
    "name": "@elgato/icons",
    "version": "0.0.0",
    "description": "Icons used throughout the Elgato ecosystem.",
    "type": "module",
    "exports": {
        "./s": {
            "default": "./dist/strings/s/index.js",
            "types": "./dist/strings/s/index.d.ts"
        },
        "./m": {
            "default": "./dist/strings/m/index.js",
            "types": "./dist/strings/m/index.d.ts"
        },
        "./l": {
            "default": "./dist/strings/l/index.js",
            "types": "./dist/strings/l/index.d.ts"
        },
        "./metadata": {
            "default": "./dist/metadata/index.js",
            "types": "./dist/metadata/index.d.ts"
        },
        "./react": {
            "default": "./dist/react/index.js",
            "types": "./dist/react/index.d.ts"
        }
    },
    "files": [
        "dist/",
        "svg/"
    ],
    "scripts": {
        "build": "npm run sync-icons && tsc",
        "sync-icons": "npm run build && node dist/scripts/sync-icons.js",
        "build:icons": "tsx scripts/sync-icons.ts",
        "transform": "tsx scripts/transform.ts"
    },
    "keywords": [
        "elgato",
        "icons",
        "stream deck",
        "wave link",
        "camera hub",
        "marketplace",
        "makers"
    ],
    "author": {
        "name": "Elgato",
        "url": "https://www.elgato.com"
    },
    "license": "MIT",
    "homepage": "https://docs.elgato.com/icons",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/elgatosf/icons.git"
    },
    "prettier": "@elgato/prettier-config",
    "dependencies": {
        "axios": "^1.6.5",
        "dotenv": "^16.3.1",
        "figma-api": "^1.11.0",
        "svgo": "^3.0.0"
    },
    "devDependencies": {
        "@elgato/prettier-config": "^0.2.4",
        "@svgr/core": "^8.1.0",
        "@svgr/plugin-jsx": "^8.1.0",
        "@svgr/plugin-svgo": "^8.1.0",
        "@tsconfig/node20": "^20.1.4",
        "@types/node": "^20.17.10",
        "@types/react": "^18.3.12",
        "@vercel/node": "^3.0.0",
        "ora": "^8.1.1",
        "react": "^18.3.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2"
    }
}
