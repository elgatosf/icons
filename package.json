{
    "name": "@elgato/icons",
    "version": "0.1.0",
    "description": "Icons used throughout the Elgato ecosystem.",
    "type": "module",
    "exports": {
        ".": {
            "default": "./dist/strings/index.js",
            "types": "./dist/strings/index.d.ts"
        },
        "./s": {
            "default": "./dist/strings/s/index.js",
            "types": "./dist/strings/s/index.d.ts"
        },
        "./m": {
            "default": "./dist/strings/m/index.js",
            "types": "./dist/strings/m/index.d.ts"
        },
        "./l": {
            "default": "./dist/strings/l/index.js",
            "types": "./dist/strings/l/index.d.ts"
        }
    },
    "files": [
        "dist/",
        "svg/"
    ],
    "scripts": {
        "build": "npm run build:transform && npm run lint:fix && npm run tsc",
        "build:ci": "tsc",
        "build:transform": "node --experimental-strip-types scripts/transform.ts",
        "tsc": "tsc",
        "lint": "prettier . --check",
        "lint:fix": "prettier . --write"
    },
    "keywords": [
        "elgato",
        "icons",
        "stream deck",
        "wave link",
        "camera hub"
    ],
    "author": {
        "name": "Elgato",
        "url": "https://www.elgato.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/elgatosf/icons.git"
    },
    "prettier": "@elgato/prettier-config",
    "devDependencies": {
        "@elgato/prettier-config": "^0.2.4",
        "@tsconfig/node20": "^20.1.4",
        "@types/node": "^22.8.6",
        "ora": "^8.1.1",
        "typescript": "^5.6.3"
    }
}
